<div id="general" class="section">
    <h2>General</h2>
    <div class="form-group"><label class="label">Prefix (Max 3 chars)</label><input type="text" id="prefix" value="<%= settings.prefix||'!' %>" maxlength="3"></div>
    <div class="form-group">
        <label class="label">Delete command execution messages</label>
        <label class="switch">
            <input type="checkbox" id="delete_prefix_cmd_message" <%= settings.delete_prefix_cmd_message ? 'checked' : '' %>>
            <span class="slider"></span>
        </label>
        <small style="display: block; margin-top: 10px; color: #999;">Automatically delete the prefix command message after execution</small>
    </div>
</div>
